<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.edrkr">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />


    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <!-- http 통신 트레픽 허용 -->

        <activity android:name=".firstpage.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".mainpage.MonitoringPage"
            android:theme="@style/AppTheme.NoActionBar"
            ></activity>

        <activity android:name=".firstpage.login_SingUpPage"
            android:theme="@style/AppTheme.NoActionBar"></activity>

        <activity android:name=".KeyCreatePage"></activity>
        <!--이아름 병합-->
        <activity android:name=".SelectArea"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".SelectMember"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".show_each_areahas"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".show_each_member"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Managerpage"
            android:theme="@style/AppTheme.NoActionBar" /> <!-- http 통신 트레픽 허용 -->
        <activity android:name=".show_each_board"
        android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NoticeBoardActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".WritingActivity"
            android:theme="@style/AppTheme.NoActionBar" />

    </application>

</manifest>